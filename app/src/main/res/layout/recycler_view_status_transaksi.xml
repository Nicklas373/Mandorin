<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="160dp"
    card_view:cardCornerRadius="6dp"
    card_view:cardElevation="3dp"
    android:id="@+id/cv_head"
    card_view:cardUseCompatPadding="true"
    android:clickable="true"
    android:focusable="true"
    android:foreground="?android:attr/selectableItemBackground">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent">

        <android.support.v7.widget.CardView
            android:layout_width="wrap_content"
            android:id="@+id/cv_title"
            android:layout_height="35dp"
            card_view:cardUseCompatPadding="true"
            app:cardBackgroundColor="#0069c0">

            <TextView
                android:id="@+id/TextViewTitle"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:text="@string/app_mandor_recycler_status_pembayaran_renovasi_4"
                android:fontFamily="@font/test"
                android:textColor="#ffffff"
                android:textSize="15sp" />

        </android.support.v7.widget.CardView>

        <TextView
            android:id="@+id/text_nomor_kontrak_status_transaksi"
            android:layout_width="110dp"
            android:layout_marginStart="10dp"
            android:layout_height="wrap_content"
            android:text="@string/app_mandor_recycler_status_pembayaran_renovasi_1"
            android:fontFamily="@font/test"
            android:layout_below="@+id/cv_title"
            android:textColor="#000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/heck_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/text_nomor_kontrak_status_transaksi"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/test"
            android:layout_below="@+id/cv_title"
            android:text="@string/heck"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/text_nomor_kontrak_status_transaksi_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/text_nomor_kontrak_status_transaksi"
            android:layout_marginStart="30dp"
            android:layout_below="@+id/cv_title"
            android:text=""
            android:fontFamily="@font/test"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/text_biaya_status_transaksi"
            android:layout_width="110dp"
            android:layout_marginStart="10dp"
            android:layout_height="wrap_content"
            android:text="@string/app_mandor_recycler_status_pembayaran_renovasi_2"
            android:fontFamily="@font/test"
            android:layout_below="@+id/text_nomor_kontrak_status_transaksi"
            android:layout_marginTop="10dp"
            android:textColor="#000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/heck_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/text_biaya_status_transaksi"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/test"
            android:layout_below="@+id/text_nomor_kontrak_status_transaksi"
            android:text="@string/heck"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/text_biaya_status_transaksi_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/text_biaya_status_transaksi"
            android:layout_marginStart="30dp"
            android:fontFamily="@font/test"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/text_nomor_kontrak_status_transaksi"
            android:text=""
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/text_alamat_status_transaksi"
            android:layout_width="110dp"
            android:layout_marginStart="10dp"
            android:layout_height="wrap_content"
            android:text="@string/app_mandor_recycler_status_pembayaran_renovasi_3"
            android:fontFamily="@font/test"
            android:layout_below="@+id/text_biaya_status_transaksi"
            android:layout_marginTop="10dp"
            android:textColor="#000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/heck_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/text_alamat_status_transaksi"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/test"
            android:layout_below="@+id/text_biaya_status_transaksi"
            android:text="@string/heck"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/text_alamat_status_transaksi_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/text_alamat_status_transaksi"
            android:layout_marginStart="30dp"
            android:fontFamily="@font/test"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/text_biaya_status_transaksi"
            android:text=""
            android:textColor="#000000"
            android:textSize="15sp" />

    </RelativeLayout>

</android.support.v7.widget.CardView>