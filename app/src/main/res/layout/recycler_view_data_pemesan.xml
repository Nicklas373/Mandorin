<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="210dp"
    card_view:cardCornerRadius="6dp"
    card_view:cardElevation="3dp"
    android:id="@+id/cv_head"
    card_view:cardUseCompatPadding="true"
    android:clickable="true"
    android:focusable="true"
    android:foreground="?android:attr/selectableItemBackground">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent">

        <android.support.v7.widget.CardView
            android:id="@+id/cv_title"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            app:cardBackgroundColor="#0069c0"
            card_view:cardElevation="3dp"
            card_view:cardUseCompatPadding="true">

            <TextView
                android:id="@+id/TextViewTitle"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/test"
                android:layout_marginStart="3dp"
                android:text="@string/app_mandor_recycler_pemesan_renovasi_6"
                android:textColor="#ffffff"
                android:textSize="15sp" />

        </android.support.v7.widget.CardView>

        <TextView
            android:id="@+id/text_id_data_pemesan"
            android:layout_width="114dp"
            android:layout_marginStart="10dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/cv_title"
            android:fontFamily="@font/test"
            android:text="@string/app_mandor_recycler_data_pemesan_1"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/heck_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/cv_title"
            android:fontFamily="@font/test"
            android:layout_marginStart="45dp"
            android:layout_toEndOf="@+id/text_id_data_pemesan"
            android:text="@string/heck"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/text_id_data_pemesan_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/cv_title"
            android:layout_marginStart="60dp"
            android:layout_toEndOf="@+id/text_id_data_pemesan"
            android:text=""
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/nomor_pemesan_data_pemesan"
            android:layout_marginStart="10dp"
            android:layout_width="114dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/test"
            android:layout_below="@+id/text_id_data_pemesan"
            android:layout_marginTop="10dp"
            android:text="@string/app_mandor_recycler_data_pemesan_2"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/heck_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/text_id_data_pemesan"
            android:layout_marginStart="45dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/test"
            android:layout_toEndOf="@+id/nomor_pemesan_data_pemesan"
            android:text="@string/heck"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/nomor_pemesan_data_pemesan_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/text_id_data_pemesan"
            android:layout_marginStart="60dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/test"
            android:layout_toEndOf="@+id/nomor_pemesan_data_pemesan"
            android:text=""
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/tgl_survey_pemesan"
            android:layout_marginStart="10dp"
            android:layout_width="145dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/test"
            android:layout_below="@+id/nomor_pemesan_data_pemesan"
            android:layout_marginTop="10dp"
            android:text="@string/app_mandor_recycler_data_pemesan_3"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/heck_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/test"
            android:layout_below="@+id/nomor_pemesan_data_pemesan"
            android:layout_marginStart="14dp"
            android:layout_marginTop="10dp"
            android:layout_toEndOf="@+id/tgl_survey_pemesan"
            android:text="@string/heck"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/tgl_survey_pemesan_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/test"
            android:layout_below="@+id/nomor_pemesan_data_pemesan"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:layout_toEndOf="@+id/tgl_survey_pemesan"
            android:text=""
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/status_pemesan"
            android:layout_marginStart="10dp"
            android:layout_width="145dp"
            android:fontFamily="@font/test"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tgl_survey_pemesan"
            android:layout_marginTop="10dp"
            android:text="@string/app_mandor_recycler_data_pemesan_4"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/heck_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tgl_survey_pemesan"
            android:layout_marginStart="14dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/test"
            android:layout_toEndOf="@+id/status_pemesan"
            android:text="@string/heck"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/status_pemesan_1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_below="@+id/tgl_survey_pemesan"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/test"
            android:layout_toEndOf="@+id/status_pemesan"
            android:text=""
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/alamat_pemesan"
            android:layout_marginStart="10dp"
            android:layout_width="145dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/test"
            android:layout_below="@+id/status_pemesan"
            android:layout_marginTop="10dp"
            android:text="@string/app_mandor_recycler_data_pemesan_5"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/heck_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/status_pemesan"
            android:layout_marginStart="14dp"
            android:fontFamily="@font/test"
            android:layout_marginTop="10dp"
            android:layout_toEndOf="@+id/alamat_pemesan"
            android:text="@string/heck"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/alamat_pemesan_1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_below="@+id/status_pemesan"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/test"
            android:layout_toEndOf="@+id/alamat_pemesan"
            android:text=""
            android:textColor="#000000"
            android:textSize="15sp" />

    </RelativeLayout>

</android.support.v7.widget.CardView>